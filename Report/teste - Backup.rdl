      <Query>
        <DataSourceName>ds_EPT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        u.NomeCompleto, u.Email
FROM            Pessoa AS p INNER JOIN
                         Usuario AS u ON p.UsuarioId = u.UsuarioId
WHERE        (u.UsuarioId = @UsuarioId)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@UsuarioId">
            <Value>=Parameters!UsuarioId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
